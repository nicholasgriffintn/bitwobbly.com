{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "bitwobbly-scheduler",
  "main": "src/index.ts",
  "compatibility_date": "2025-01-01",
  "dev": {
    "port": 8788,
    "inspector_port": 9230
  },
  "vars": {
    "PUBLIC_TEAM_ID": "team_demo"
  },
  "triggers": {
    "crons": ["* * * * *"]
  },
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "bitwobbly_db",
      "database_id": "REPLACE_ME",
      "migrations_dir": "../../packages/shared/drizzle"
    }
  ],
  "queues": {
    "producers": [
      {
        "binding": "CHECK_JOBS",
        "queue": "bitwobbly_check_jobs"
      }
    ]
  }
}
