{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "bitwobbly-checker",
  "main": "src/index.ts",
  "compatibility_date": "2025-01-01",
  "dev": {
    "port": 8787,
    "inspector_port": 9231
  },
  "vars": {
    "PUBLIC_TEAM_ID": "team_demo"
  },
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "bitwobbly_db",
      "database_id": "9bd1c8a6-c792-4ecd-bf3d-d787bf1c9b17",
      "migrations_dir": "../../packages/shared/drizzle"
    }
  ],
  "kv_namespaces": [
    {
      "binding": "KV",
      "id": "7b37c9a568bf44558ba6959148073058"
    }
  ],
  "queues": {
    "consumers": [
      {
        "queue": "bitwobbly-check-jobs",
        "max_batch_size": 10,
        "max_batch_timeout": 5
      }
    ],
    "producers": [
      {
        "binding": "ALERT_JOBS",
        "queue": "bitwobbly-alert_-obs"
      }
    ]
  },
  "durable_objects": {
    "bindings": [
      {
        "name": "INCIDENT_DO",
        "class_name": "IncidentCoordinator"
      }
    ]
  },
  "migrations": [
    {
      "tag": "v1",
      "new_classes": ["IncidentCoordinator"]
    }
  ]
}
