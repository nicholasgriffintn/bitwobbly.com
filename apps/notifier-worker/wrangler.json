{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "bitwobbly-notifier",
  "main": "src/index.ts",
  "compatibility_date": "2025-01-01",
  "dev": {
    "port": 8789,
    "inspector_port": 9232
  },
  "vars": {
    "PUBLIC_TEAM_ID": "team_demo"
  },
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "bitwobbly_db",
      "database_id": "9bd1c8a6-c792-4ecd-bf3d-d787bf1c9b17",
      "migrations_dir": "../../packages/shared/drizzle"
    }
  ],
  "queues": {
    "consumers": [
      {
        "queue": "bitwobbly-alert_-obs",
        "max_batch_size": 10,
        "max_batch_timeout": 5
      }
    ]
  }
}
