{
  "name": "@bitwobbly/app-worker",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && tsc --noEmit",
    "preview": "vite preview",
    "deploy": "npm run build && wrangler deploy",
    "deploy:prod": "pnpm run deploy",
    "cf-typegen": "wrangler types",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "node --experimental-strip-types --test src/**/*.test.ts",
    "db:migrate:local": "wrangler d1 migrations apply bitwobbly_db --local --persist-to ../../.data",
    "db:migrate:remote": "wrangler d1 migrations apply bitwobbly_db --remote"
  },
  "dependencies": {
    "@bitwobbly/auth": "workspace:*",
    "@bitwobbly/shared": "workspace:*",
    "@cloudflare/vite-plugin": "^1.21.2",
    "@sentry/react": "^10.36.0",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-devtools": "^0.9.2",
    "@tanstack/react-router": "^1.157.16",
    "@tanstack/react-router-devtools": "^1.157.16",
    "@tanstack/react-router-ssr-query": "^1.157.16",
    "@tanstack/react-start": "^1.157.16",
    "@tanstack/router-plugin": "^1.157.16",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "recharts": "^3.7.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260123.0",
    "@tanstack/devtools-vite": "^0.4.1",
    "@types/node": "^25.2.0",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "drizzle-orm": "^0.45.1",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-tsconfig-paths": "^6.0.5",
    "wrangler": "^4.61.1"
  }
}
