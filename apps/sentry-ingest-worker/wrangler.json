{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "bitwobbly-sentry-ingest",
  "main": "src/index.ts",
  "compatibility_date": "2025-07-15",
  "compatibility_flags": ["nodejs_compat"],
  "routes": [
    {
      "pattern": "ingest.bitwobbly.com",
      "custom_domain": true
    }
  ],
  "workers_dev": false,
  "preview_urls": false,
  "observability": {
    "enabled": true,
    "head_sampling_rate": 0.01,
    "logs": {
      "enabled": true,
      "head_sampling_rate": 0.01,
      "invocation_logs": false
    }
  },
  "dev": {
    "port": 8789,
    "inspector_port": 9232
  },
  "vars": {
    "SENTRY_DSN": "https://0b3358d7860a4be0909f9cebdff553b7@ingest.bitwobbly.com/5"
  },
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "bitwobbly_db",
      "database_id": "f2424789-6b8b-4b06-aadd-0de982901cab"
    }
  ],
  "r2_buckets": [
    {
      "binding": "SENTRY_RAW",
      "bucket_name": "bitwobbly-issues-raw"
    }
  ],
  "pipelines": [
    {
      "binding": "SENTRY_PIPELINE",
      "pipeline": "5752a0f6331943dc80e0ffca8ccd3b6f"
    }
  ],
  "queues": {
    "producers": [
      {
        "binding": "SENTRY_EVENTS",
        "queue": "bitwobbly-sentry-events"
      }
    ]
  },
  "kv_namespaces": [
    {
      "binding": "KV",
      "id": "7b37c9a568bf44558ba6959148073058"
    }
  ],
  "ratelimits": [
    {
      "name": "SENTRY_RATE_LIMITER",
      "namespace_id": "2001",
      "simple": { "limit": 1000, "period": 60 }
    }
  ]
}
